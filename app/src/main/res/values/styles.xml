<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ButtonStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="cornerRadius">8dp</item>
        <item name="backgroundTint">@color/black</item>
        <item name="elevation">8dp</item>
    </style>

    <style name="ItemButtonStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="cornerRadius">16dp</item>
        <item name="backgroundTint">@color/black</item>
        <item name="elevation">8dp</item>
    </style>

    <style name="CornerCut" parent="">
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeBottomLeft">16sp</item>
        <item name="cornerSizeBottomRight">16sp</item>
    </style>

    <style name="Circle" parent="">
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeBottomLeft">16dp</item>
        <item name="cornerSizeBottomRight">16dp</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeTopRight">16dp</item>
    </style>

    <style name="MyTextView" parent="">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="MyTextView12" parent="">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    
</resources>